<!-- Instagram Embed Script -->
<script async src="https://www.instagram.com/embed.js"></script>
<script>
    // Ensure Instagram embeds are processed after page load
    function processEmbeds() {
        // Force remove inline styles BEFORE Instagram processes embeds
        // This ensures Instagram script uses correct dimensions from the start
        const wrappers = document.querySelectorAll('.instagram-embed-wrapper');
        wrappers.forEach((wrapper) => {
            const blockquote = wrapper.querySelector('blockquote');
            if (blockquote) {
                // Remove all inline width/min-width/max-width styles
                blockquote.removeAttribute('style');
                // Re-apply only necessary styles (background, border, etc.) but NOT width
                blockquote.style.background = '#FFF';
                blockquote.style.border = '0';
                blockquote.style.borderRadius = '3px';
                blockquote.style.boxShadow = '0 0 1px 0 rgba(0,0,0,0.5),0 1px 10px 0 rgba(0,0,0,0.15)';
                blockquote.style.margin = '0';
                blockquote.style.padding = '0';
            }
        });

        if (window.instgrm) {
            window.instgrm.Embeds.process();
            
            // Function to fix iframe dimensions
            function fixIframeDimensions() {
                const wrappers = document.querySelectorAll('.instagram-embed-wrapper');
                wrappers.forEach((wrapper) => {
                    const blockquote = wrapper.querySelector('blockquote');
                    const iframe = wrapper.querySelector('iframe');
                    if (blockquote) {
                        blockquote.style.width = '';
                        blockquote.style.minWidth = '';
                        blockquote.style.maxWidth = '';
                    }
                    if (iframe && wrapper.offsetWidth > 0) {
                        // Force iframe to match wrapper dimensions exactly
                        const targetWidth = wrapper.offsetWidth;
                        const targetHeight = wrapper.offsetHeight;
                        
                        // Remove any border that might cause 1px difference
                        iframe.style.border = '0';
                        iframe.style.padding = '0';
                        iframe.style.margin = '0';
                        iframe.style.outline = '0';
                        iframe.style.boxSizing = 'border-box';
                        iframe.style.width = targetWidth + 'px';
                        iframe.style.height = targetHeight + 'px';
                        iframe.style.maxWidth = '100%';
                        iframe.style.maxHeight = '100%';
                        
                        // If iframe is still 1px wider, use transform scale to fix it
                        if (iframe.offsetWidth > targetWidth) {
                            const scaleX = targetWidth / iframe.offsetWidth;
                            const scaleY = targetHeight / iframe.offsetHeight;
                            iframe.style.transformOrigin = 'top left';
                            iframe.style.transform = `scale(${scaleX}, ${scaleY})`;
                            iframe.style.width = (iframe.offsetWidth / scaleX) + 'px';
                            iframe.style.height = (iframe.offsetHeight / scaleY) + 'px';
                        } else {
                            iframe.style.transform = '';
                        }
                        
                        // Also set blockquote to ensure it doesn't cause issues
                        if (blockquote) {
                            blockquote.style.border = '0';
                            blockquote.style.padding = '0';
                            blockquote.style.margin = '0';
                            blockquote.style.boxSizing = 'border-box';
                        }
                    }
                });
            }
            
            // Clean up inline styles after Instagram processes embeds
            setTimeout(fixIframeDimensions, 100);
            setTimeout(fixIframeDimensions, 500);
            setTimeout(fixIframeDimensions, 1000);
            setTimeout(fixIframeDimensions, 2000);
            
            // Use MutationObserver to watch for changes
            const observer = new MutationObserver(() => {
                fixIframeDimensions();
            });
            
            wrappers.forEach((wrapper) => {
                observer.observe(wrapper, {
                    childList: true,
                    attributes: true,
                    attributeFilter: ['style', 'width', 'height'],
                    subtree: true
                });
            });
        }
    }

    if (window.instgrm) {
        processEmbeds();
    } else {
        window.addEventListener('load', function() {
            processEmbeds();
        });
    }

    // Clear blockquote inline styles IMMEDIATELY on DOMContentLoaded
    // This happens before Instagram script processes embeds
    window.addEventListener('DOMContentLoaded', () => {
        const wrappers = document.querySelectorAll('.instagram-embed-wrapper');
        wrappers.forEach((wrapper) => {
            const blockquote = wrapper.querySelector('blockquote');
            if (blockquote) {
                // Remove width-related inline styles immediately
                blockquote.style.width = '';
                blockquote.style.minWidth = '';
                blockquote.style.maxWidth = '';
            }
        });
    });
</script>
