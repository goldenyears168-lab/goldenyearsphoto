        const grid = document.getElementById('portfolio-grid');
        // Fallback to passport-korea if category not found
        const data = portfolioData[category] || portfolioData['passport-korea'];
      
        // Adjust Grid Layout based on data type - No fixed row height, let images maintain aspect ratio
        if (data.layout === 'horizontal') {
            grid.className = "grid grid-cols-1 md:grid-cols-2 gap-4 md:gap-6";
      } else {
            grid.className = "grid grid-cols-2 md:grid-cols-4 gap-4 md:gap-6";
      }

        // Render Items with Fade Effect - No cropping, preserve aspect ratio
        grid.innerHTML = data.items.map((item, index) => `
            <div class="portfolio-img-card animate-fade-in group border border-slate-100" style="animation-delay: ${index * 0.1}s">
                <img src="${item.src}" alt="${item.sub}" loading="lazy">
